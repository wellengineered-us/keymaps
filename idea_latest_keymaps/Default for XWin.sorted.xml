  </action>
  <action id="RunToCursor">
    <keyboard-shortcut first-keystroke="SHIFT ALT 9" />
  </action>
  <action id="ShowFilePath">
    <keyboard-shortcut first-keystroke="CONTROL ALT SHIFT 2" />
  </action>
  <action id="ShowSettingsAndFindUsages">
    <keyboard-shortcut first-keystroke="CONTROL SHIFT ALT 7" />
  </action>
